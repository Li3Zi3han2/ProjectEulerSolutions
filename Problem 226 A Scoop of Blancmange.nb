Notebook[{
Cell[TextData[{
 "The blancmange curve is the set of points ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"x", ",", "y"}], "}"}], TraditionalForm]],ExpressionUUID->
  "e5d3c694-48fd-440a-a554-b65121bbb03c"],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
   "0", "\[ThinSpace]", "\[LessEqual]", "\[ThinSpace]", "x", "\[ThinSpace]", 
    "\[LessEqual]", "\[ThinSpace]", "1"}], TraditionalForm]],ExpressionUUID->
  "6d5246d0-ecb3-4ee1-aab0-2a920a1d982f"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"n", "=", "0"}], 
      RowBox[{"+", "\[Infinity]"}]], 
     FractionBox[
      RowBox[{"s", "[", 
       RowBox[{
        SuperscriptBox["2", "n"], " ", "x"}], "]"}], 
      SuperscriptBox["2", "n"]]}]}], TraditionalForm]],ExpressionUUID->
  "ee854285-70ac-4e4b-86b7-b7c8f736014a"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"s", "[", "x", "]"}], "=", 
    RowBox[{"Abs", "[", 
     RowBox[{"x", "-", 
      RowBox[{"Round", "[", "x", "]"}]}], "]"}]}], TraditionalForm]],
  ExpressionUUID->"5b5fb8f1-2fda-4863-9660-4c57efa5061a"],
 ".\n\nThe area under the blancmange curve is equal to ",
